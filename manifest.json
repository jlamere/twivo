{
  "manifest_version": 2,
  "name": "Twivo",
  "description": "Tivo for Twitter - prevent spoilers and save tweets for later",
  "version": "1.0.1",
   "icons": { "16": "icon16.png",
           "48": "icon48.png",
          "128": "icon128.png" },
  "homepage_url": "https://github.com/jlamere/twivo/blob/master/README.md",
    "content_scripts": [
    {
      "matches":  ["https://twitter.com/*", "http://twitter.com/*"],
      "js": ["contentscript.js"]
    }
    ]
}